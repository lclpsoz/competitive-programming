#include <bits/stdc++.h>
using namespace std;

////////////// Prewritten code follows. Look down for solution. ////////////////
#define x first
#define y second
#define len(x) ((int)(x).size())
using pii = pair<int, int>;
using ll = long long;
using llu = long long unsigned;
using ld = long double;

const ld EPS = 1e-9;
inline int cmp(ld x, ld y = 0, ld tol = EPS) {
  return (x <= y + tol) ? (x + tol < y) ? -1 : 0 : 1;
}

const int MOD = 1;
inline int mod (ll x, int m = MOD) {
  return (int)(((x%m) + m)%m);
}

////////////////////////// Solution starts below. //////////////////////////////

using pp = pair<float, pair<float, int>>;
const int N =2e4+10;

int n;
int p;
float d, c, r;
vector<pp> vec;
pp dp[N];

int main () {
  while (scanf ("%d %f", &n, &d) != EOF && n) {
    for (int i = 0; i < N; i++)
      dp[i] = {-1, {-1, -1}};
    dp[0] = {d, {0, 0}};
    vec.clear();
    float avr = 0, he = 0;
    
    for (int k = 0; k < n; k++) {
      scanf ("%d %f %f", &p, &c, &r);
      for (int i = N-1; i >= p; i--)
	if (dp[i-p].x != -1 && cmp (dp[i-p].x, c) >= 0) {
	  //printf ("|%d %Lf\n", i-p, dp[i-p].y.x);
	  dp[i] = {dp[i-p].x-c,
		   {dp[i-p].y.x + r, dp[i-p].y.y + 1} };
	}
      avr += r;
    }
    for (int i = N-1; i > 0; i--)
      if (dp[i].x != -1) {
	//printf("%Lf %d\n", dp[i].y.x, dp[i].y.y);
	he = dp[i].y.x / dp[i].y.y;
	//printf ("  %Lf\n", he);
	break;
      }
    avr /= n;
    if (cmp (he, avr) < 1)
      printf ("%0.2f NO!\n", avr);
    else
      printf ("%0.2f\n", he);
  }
  
  return 0;
}
