#include <stdio.h>

int n, q;
const int N = 1e5+10;
int pref[512][N];

int mdc (int a, int b) {
  if (b == 0) return a;
  return mdc (b, a%b);
}

int mmc (int a, int b) {
  return (a * b)/mdc (a, b);
}

int main () {
  int mc = 1;
  for (int i = 3; i <= 10; i+=2) {
    mc = mmc (i, mc);
    printf ("%d: %d\n", i, mc);
  }
  
  int t, x;
  int l, r, s;
  int ans;
  scanf ("%d", &t);
  while (t--) {
    scanf ("%d %d", &n, &q);
    for (int i = 1; i <= n; i++) {
      scanf ("%d", &x);
      int mask = 0;
      for (int j = 2; j <= 10; j++)
	if (x % j == 0)
	  mask |= (1 << (j-2));
      for (int p = 1; p < 512; p++) {
	pref[p][i] = pref[p][i-1];
	if (mask & p)
	  ++pref[p][i];
      }
    }

    while (q--) {
      ans = 0;
      scanf ("%d %d %d", &l, &r, &s);
      if (s&1)
	ans = r-l+1;
      else {
	s >>= 1;
	ans = pref[s][r] - pref[s][l-1];
      }
      printf ("%d\n", ans);
    }
  }
  
  return 0;
}
