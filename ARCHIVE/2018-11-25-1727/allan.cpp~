#include <bits/stdc++.h>
using namespace std; 

const int N = 1e5+100;

int n, k, test_cases;
set<int> excluded; 
int numbers[N];

int main(){
  scanf("%d", &test_cases); 
	
  while (test_cases--){
    scanf("%d %d", &n, &k); 
    if (n < k)
      printf("%d\n", n-1); 
    else{
      // resetting array 
      for (int i = 2; i <= n; i++)
	numbers[i] = 1;

      cout << "here\n";
      for (int i = 2; i <= k; i++)
	if (numbers[i]){
	  numbers[i] = 1; 
	  for (int j = i*i; j <= n; j += i)
	    numbers[j] = 0;
	}

      cout << "TWO\n";
			
      for (int i = k+1; i <= n; i++)
	if (numbers[i]){
	  excluded.insert(i); 
	  for (int j = i+i; j <= n; j += i){
	    excluded.insert(j); 
	  }
	}
			
      cout << n-excluded.size()-1 << endl; 
      excluded.clear(); 		
    }
  }
	
  return 0; 
}
