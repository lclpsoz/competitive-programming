#include <bits/stdc++.h>
using namespace std;

////////////// Prewritten code follows. Look down for solution. ////////////////
#define x first
#define y second
#define len(x) ((int)(x).size())
using pii = pair<int, int>;
using ll = long long;
using llu = long long unsigned;
using ld = long double;

const ld EPS = 1e-9;
inline int cmp(ld x, ld y = 0, ld tol = EPS) {
  return (x <= y + tol) ? (x + tol < y) ? -1 : 0 : 1;
}

const int MOD = 1;
inline int mod (ll x, int m = MOD) {
  return (int)(((x%m) + m)%m);
}

////////////////////////// Solution starts below. //////////////////////////////

const int X = 1e6+10;
const int MASK = 1<<18;
const int N = 20;

int n, m;
int not_prime[X];
vector<int> caves;
int dist[110][110];
int dp[MASK][N];

int func (int mask, int p) {
  int &ret = dp[mask][p];
  if (ret != -1)
    return ret;
  if (__builtin_popcount (mask) == len (caves))
    return ret = dist[p][n];
  ret = 1e9;
  for (int i = 0; i < len (caves); i++)
    if (!(1<<i & mask))
      ret = min (ret,
		 func (mask | 1<<i, caves[i]) + dist[p][caves[i]]);

  return ret;
}

int main () {
  not_prime[1] = not_prime[0] = true;
  // Sieve of Eratosthenes
  for (int i = 2; i*i < X; i++)
    if (!not_prime[i])
      for (int j = i*i; j < X; j+=i)
	not_prime[j] = true;
  
  while (scanf ("%d %d", &n, &m) != EOF) {
    assert (n <= 100 && m <= n*n); // <------------------- ASSERT
    ++n;
    for (int i = 1; i <= n; i++)
      for (int j = 1; j <= n; j++)
	if (i != j)
	  dist[i][j] = 1e9;
	else
	  dist[i][j] = 0;
    
    for (int i = 1; i < n; i++) { 
      int x;
      scanf ("%d", &x);
      assert (x <= (int)1e5); // <----------- ASSERT
      if (!not_prime[x])
	caves.push_back (i+1);
    }
    
    while (m--) {
      int u, v, w;
      scanf ("%d %d %d", &u, &v, &w);
      dist[u][v] = dist[v][u] = min (dist[v][u], min (dist[u][v], w));
    }

    // Floyd-Warshall
    for (int k = 1; k <= n; k++)
      for (int i = 1; i <= n; i++)
	for (int j = 1; j <= n; j++)
	  dist[j][i] = dist[i][j] = min (dist[j][i], min (dist[i][j], dist[i][k] + dist[k][j]));

    // Hamiltonian Path
    memset (dp, -1, sizeof dp);
    int ans = 2e9;
    for (int i = 0; i < len (caves); i++)
      ans = min (ans, func (1<<i, caves[i])) + dist[caves[i]][n];
    
    printf ("%d\n", ans);
      
    caves.clear();
  }
  
  return 0;
}
